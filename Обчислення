#include <iostream> //i/o/library
using namespace std; //використання пространства імен, аби не писати постійно

int main() //заголовок основної функції
{ //початок виконання задачі
	int a;
	cout << "Enter value a : ";
	cin >> a;
	int res = a - 5; // обчислення задачі
	cout << "a-5: "<< res; //виведення результату
	return 0;
} //кінець виконаного завдання
